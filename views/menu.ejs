
<%- include ("templates/encabezado")%> 

    <body style="background-color: rgba(165, 156, 130, 0.904);">

        
        <!-- Page Header Start -->
        
        <div class="page-header mb-0">

            <div class="container">

                <div class="row">
                    <a href="/" class="nav-item nav-link active">Inicio</a> 
                    <a href="/cliente/comprar" class="nav-item nav-link">Comprar</a>
                    <a href="/cliente/carrito" class="nav-item nav-link">Carrito</a>  
 
                </div>
            </div>
        </div>
        <!-- Page Header End -->
        
        
        <!-- Food Start -->
        <div class="food mt-0">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-md-4">
                        <div class="food-item" style="background: rgb(247, 74, 52);">
                            <i class="flaticon-burger"></i>
                            <h2>Hamburgesas</h2>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="food-item"style="background: rgb(247, 74, 52);">
                            <i class="flaticon-snack"></i>
                            <h2>Especiales</h2>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="food-item" style="background: rgb(247, 74, 52);">
                            <i class="flaticon-cocktail"></i>
                            <h2>Bebidas</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Food End -->
        

        <!-- Menu Start -->
        <div class="menu">
            <div class="container">
                <div class="section-header text-center">
                    <h2>Men√∫</h2>
                </div>
                <div class="menu-tab">
                    <ul class="nav nav-pills justify-content-center">
                        <li class="nav-item">
                            <a class="nav-link active" data-toggle="pill" href="#burgers">Hamburgesas</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="pill" href="#snacks">Especiales</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="pill" href="#beverages">Bebidas</a>
                        </li>
                    </ul>
                    <div class="tab-content">
                        <div id="burgers" class="container tab-pane active">
                            <div class="row">
                                <% 
                                    producto.forEach((producto)=>{
                                %>
                                <div class="col-lg-7 col-md-12">
                                    <div class="menu-item">
                                        <div class="menu-img">
                                            <img src="<%= producto.foto%>" alt="Image">
                                        </div>
                                        <div class="menu-text">
                                            <h3 style="color: #fbaf32;">
                                                <span><%= producto.nombre%></span> 
                                                <strong>$ <%= producto.precio%></strong>    
                                            </h3>
                                            <p><%= producto.descripcion%></p>
                                        </div>
                                    </div>
                                    
                                </div>

                                <% 
                                    }); 
                                %>
                                <div class="col-lg-5 d-none d-lg-block">
                                    <img src="img/menu-burger-img.jpg" alt="Image">
                                </div>
                            </div>
                        </div>




                        <div id="snacks" class="container tab-pane fade">
                            <div class="row">
                                <% 
                                    especial.forEach((especial)=>{
                                %>
                                <div class="col-lg-7 col-md-12">
                                    <div class="menu-item">
                                        <div class="menu-img">
                                            <img src="<%= especial.foto%>" alt="Image">
                                        </div>
                                        <div class="menu-text">
                                            <h3><span><%= especial.nombre%></span> <strong>$ <%= especial.precio%></strong></h3>
                                            <p><%= especial.descripcion%></p>
                                        </div>
                                    </div>
                                    
                                </div>

                                <% 
                                    }); 
                                %>
                                <div class="col-lg-5 d-none d-lg-block">
                                    <img src="img/menu-snack-img.jpg" alt="Image">
                                </div>
                            </div>
                        </div>
                        <div id="beverages" class="container tab-pane fade">
                            <div class="row">
                                <% 
                                bebidas.forEach((bebidas)=>{
                            %>
                            <div class="col-lg-7 col-md-12">
                                <div class="menu-item">
                                    <div class="menu-img">
                                        <img src="<%= bebidas.foto%>" alt="Image">
                                    </div>
                                    <div class="menu-text">
                                        <h3><span><%= bebidas.nombre%></span> <strong>$ <%= bebidas.precio%></strong></h3>
                                        <p><%= bebidas.descripcion%></p>
                                    </div>
                                </div>
                                
                            </div>

                            <% 
                                }); 
                            %>
                                <div class="col-lg-5 d-none d-lg-block">
                                    <img src="img/menu-beverage-img.jpg" alt="Image">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Menu End -->



        <!-- JavaScript Libraries -->
        <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
        <script src="lib/easing/easing.min.js"></script>
        <script src="lib/owlcarousel/owl.carousel.min.js"></script>
        <script src="lib/tempusdominus/js/moment.min.js"></script>
        <script src="lib/tempusdominus/js/moment-timezone.min.js"></script>
        <script src="lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>
        
        <!-- Contact Javascript File -->
        <script src="mail/jqBootstrapValidation.min.js"></script>
        <script src="mail/contact.js"></script>

        <!-- Template Javascript -->
        <script src="js/main.js"></script>

<%- include ("templates/pie")%>